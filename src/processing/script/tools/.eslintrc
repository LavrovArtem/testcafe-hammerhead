{
  "rules": {
    "no-alert": 0,
    "no-array-constructor": 0,
    "no-caller": 0,
    "no-catch-shadow": 0,
    "no-empty-label": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-implied-eval": 0,
    "no-iterator": 0,
    "no-label-var": 0,
    "no-labels": 0,
    "no-lone-blocks": 0,
    "no-loop-func": 0,
    "no-multi-str": 0,
    "no-native-reassign": 0,
    "no-new": 0,
    "no-new-func": 0,
    "no-new-object": 0,
    "no-new-wrappers": 0,
    "no-octal-escape": 0,
    "no-process-exit": 0,
    "no-proto": 0,
    "no-return-assign": 0,
    "no-script-url": 0,
    "no-sequences": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-spaced-func": 0,
    "no-trailing-spaces": 0,
    "no-undef-init": 0,
    "no-unused-expressions": 0,
    "no-with": 0,
    "camelcase": 0,
    "comma-spacing": 0,
    "consistent-return": 0,
    "eqeqeq": 0,
    "semi": 0,
    "semi-spacing": 0,
    "space-infix-ops": 0,
    "space-return-throw-case": 0,
    "space-unary-ops": 0,
    "yoda": 0,
    "brace-style": 0,
    "eol-last": 0,
    "indent": 0,
    "key-spacing": 0,
    "max-nested-callbacks": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "no-lonely-if": 0,
    "no-multiple-empty-lines":  0,
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "object-curly-spacing": 0,
    "operator-assignment": 0,
    "quotes": 0,
    "space-after-keywords": 0,
    "space-before-blocks": 0,
    "prefer-const": 0,
    "no-path-concat": 0,
    "no-undefined": 0,
    "strict": 0,
    "curly": 0,
    "dot-notation": 0,
    "no-else-return": 0,
    "one-var": 0,
    "no-multi-spaces": 0,
    "radix": 0,
    "no-extra-parens": 0,
    "new-cap": 0,
    "space-before-function-paren": 0,
    "no-use-before-define" : 0,
    "handle-callback-err": 0,
    "no-fallthrough": 0,
    "no-undef": 0,
    "no-cond-assign": 0,
    "babel/arrow-parens": 0,
    "no-unused-vars": 0,
    "no-redeclare": 0,
    "comma-dangle": 0,
    "prefer-arrow-callback": 0
  }
}
